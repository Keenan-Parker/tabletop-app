<p id="notice"><%= notice %></p>

<h1>Rooms</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>GM</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rooms.each do |room| %>
      <tr>
        <td><%= room.name %></td>
        <td><%= room.gm_name %></td>
        <td><%= link_to 'View', room %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Create a room', new_room_path %>
